(script jazz

(import (jazz.debuggee.stub)
        (jazz.network)
        (jazz.presence))

(start-remote-presence purpose: 'debugging)

(let ((register (connect-remote-register localhost 60000 purpose: 'debugging)))
  (let ((debuggee-process (find-object register 'debuggee)))
    (tell (get-threads debuggee-process))))

(sleep +inf.0))
